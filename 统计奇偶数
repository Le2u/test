#include <stdio.h>

int main(int argc, const char * argv[]) {
    // insert code here...
    int a = 0 , b = 0 ;
    int i ;
    
    
    while (i != -1){
        scanf("%d",&i );
        if (i < 100000 && i >= 0) {
            if ((i % 2) == 0 ) {
                a++ ;
            }else{
                b++ ;
            }
        }
    }
    
    printf("%d %d\n", b , a);
    
    return 0;
}
