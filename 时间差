#include <stdio.h>

int main(int argc, const char * argv[]){
    
    
    int hour1 , minter1 ;
    int hour2 , minter2 ;
    
    printf("请输入时间一：(小时，分钟)\n");
    scanf("%d,%d",&hour1,&minter1);
    printf("请输入时间二：(小时，分钟)\n");
    scanf("%d,%d",&hour2,&minter2);
    
    int t1 = hour1 * 60 + minter1 ;
    int t2 = hour2 * 60 + minter2 ;
    
    int t = t2 - t1 ;
    
    printf("相差%d小时%d分钟\n", t/60 , t%60);
}
